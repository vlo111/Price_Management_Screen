@model Dictionary<int, string>
@{
    Layout = null;
}
<div class="modal-content">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 class="modal-title">Delete Parts</h3>
        <h4 style="color:crimson">Are you sure you want to delete this?</h4>
    </div>
    <div class="modal-body">
        <dl class="dl-horizontal">
            @if (Model != null)
            {
                foreach (var Part in Model)
                {
                    <dt>
                        @Html.DisplayName("Part" + " - ")
                    </dt>

                    <dd>
                        @Html.DisplayName(Part.Value)
                    </dd>
                }
            }

        </dl>
    </div>
    <div class="modal-footer">
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button class="btn btn-default" data-dismiss="modal">Close</button>
                <input id="deleteRows" type="submit" value="Delete" class="btn btn-danger" />
            </div>
        </div>
    </div>
</div>